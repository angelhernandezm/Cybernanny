<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="CyberNanny_cloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="CyberNanny_webRole" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="DefaultBinding" endpointName="HttpIn" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="HttpIn" protocol="http" port="80" />
    </Endpoints>
    <ConfigurationSettings>
      <Setting name="DataConnectionString" />
    </ConfigurationSettings>
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
      <Import moduleName="Connect" />
    </Imports>
  </WebRole>
</ServiceDefinition>